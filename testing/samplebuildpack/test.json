{
  "name":"testbuildpack",

  "config":{
    "cache": true,
    "runtime": true
  },
  "environment":[{
    "key": "SOMEENV",
    "default": "SOMEDEFVAL"
  }],
  "detect": {
    "exists": [{
      "path": "somefile.txt"
    }]
  },



  "build":[{
    "remote": [{
      "url": "https://sh.rustup.rs",
      "to": "rustup.sh",
      "to_dir": "layer"
    }],
    "local": [],
    "scripts": [{
      "command": "touch somefile"
    }]
  }]
}
